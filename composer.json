{
    "name": "rubb1/skins",
    "type": "typo3-cms-extension",
    "description": "This extension adds a dark skin (dark theme) to the TYPO3 backend. The colors are customizable and can be changed to your favorite colors.",
    "version": "10.5.2",
    "keywords": [
        "TYPO3",
        "extension",
        "backend",
        "skin",
        "dark",
        "mode",
        "darkmode",
        "custom",
        "save-and-close"
    ],
    "homepage": "https://www.myc3.com/",
    "authors": [
        {
            "name": "Dominik Schüßler",
            "role": "Developer",
            "homepage": "https://www.myc3.com/"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rubb1\\Skins\\": "Classes/"
        }
    },
    "require": {
        "typo3/cms-core": "^10.4.0 || ^11.5.0"
    },
    "extra": {
        "tags": "Stable, PHP 7.2, PHP 7.3, PHP 7.4, PHP 8.0, TYPO3 9.5, TYPO3 10.4, TYPO3 11.5, English, OpenSource, Free, TER",
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web",
            "extension-key": "skins"
        }
    }
}
